{
    environment: {
        work_dir: "embeddings_validation.work"
    }
    report_file: "../results/scenario_x5_baselines.txt"

    target: {
        file_name: "../data/clients_target.csv"
        cols_id: [client_id]
        cols_id_type: [str]
        col_target: age
    }

    split: {
        fit_ids: true

        train_id: {
            file_name: "../data/clients_target.csv"
        }
        test_id: {
            file_name: "../data/test_ids.csv"
        }

        cv_split_count: 5
        is_stratify: true
        random_state: 42
    }

    features: {
        mles_embeddings: {
            enabled: true
            read_params: {file_name: ../data/mles_embeddings.pickle}
            target_options: {}
        }
        sop_embeddings: {
            enabled: true
            read_params: {file_name: ../data/sop_embeddings.pickle}
            target_options: {}
        }
        nsp_embeddings: {
            enabled: true
            read_params: {file_name: ../data/nsp_embeddings.pickle}
            target_options: {}
        }
        rtd_embeddings: {
            enabled: true
            read_params: {file_name: ../data/rtd_embeddings.pickle}
            target_options: {}
        }
    }

    external_scores: {
        rtd_finetuning: "../results/rtd_finetuning_results.json"
    }

    models: {
        lgbm: {
            enabled: true
            cls_name: lightgbm.LGBMClassifier,
            params: {
                n_estimators=1000,
                boosting_type="gbdt",
                objective="multiclass",
                num_class=4,
                metric="multi_error",
                subsample=0.5,
                subsample_freq=1,
                learning_rate=0.05,
                feature_fraction=0.75, colsample_bytree=null,
                max_depth=6,
                lambda_l1=1, reg_alpha=null,
                lambda_l2=1, reg_lambda=null,
                min_data_in_leaf=50, min_child_samples=null,
                num_leaves=50,
                random_state=42,
                n_jobs=6,
            },
            cpu_count: 6
            preprocessing: [
                [sklearn.preprocessing.MaxAbsScaler, {}]
            ]
        }
    }

    metrics: {
        accuracy: {
            enabled: true
            score_func: sklearn.metrics.accuracy_score
            scorer_params: {needs_proba: false}
        }
    }

    report: {
        is_check_train: false
        error_handling: fail
        print_all_metrics: false

        metrics: {
            accuracy: {float_format: "{:.3f}", baseline_key=["lgbm", "baseline"], split_columns: true}
        }
    }
}
