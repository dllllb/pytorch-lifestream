    models: {
        lgbm: {
            enabled: true
            cls_name: lightgbm.LGBMClassifier,
            params: {
                n_estimators=1000,
                boosting_type="gbdt",
                objective="multiclass",
                num_class=4,
                metric="multi_error",
                subsample=0.5,
                subsample_freq=1,
                learning_rate=0.05,
                feature_fraction=0.75, colsample_bytree=null,
                max_depth=6,
                lambda_l1=1, reg_alpha=null,
                lambda_l2=1, reg_lambda=null,
                min_data_in_leaf=50, min_child_samples=null,
                num_leaves=50,
                random_state=42,
                n_jobs=6,
            },
            cpu_count: 6
            preprocessing: [
                [sklearn.preprocessing.MaxAbsScaler, {}]
            ]
        }
    }

    metrics: {
        accuracy: {
            enabled: true
            score_func: sklearn.metrics.accuracy_score
            scorer_params: {needs_proba: false}
        }
    }

    report: {
        is_check_train: true
        error_handling: fail
        print_all_metrics: false

        metrics: {
            accuracy: {float_format: "{:.3f}", split_columns: true}
