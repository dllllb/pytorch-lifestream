{
  "col_target_name": "target_age",

  "params": {
    "device": "cuda",

    "model_type": "rnn",
    "ensemble_size": 1,
    "trx_encoder": {
      "norm_embeddings": false,
      "embeddings_noise": 0.003,
      "embeddings": {
        "level_3": {"in": 200, "out": 4},
        "level_4": {"in": 800, "out": 4},
        "segment_id": {"in": 120, "out": 4}
      },
      "numeric_values": {
        "trn_sum_from_iss": "identity",
        "netto": "identity",
        "regular_points_received": "identity",
      }
    },
    "transf": {
      "train_starter": true,
      "shared_layers": false,
      "input_size": 16,
      "n_heads": 4,
      "dim_hidden": 64,
      "dropout": 0.01,
      "n_layers": 4,
      "use_positional_encoding": false,
      "max_seq_len": 800,
      "use_after_mask": false,
      "use_src_key_padding_mask": false
    },
    "rnn": {
      "type": "gru",
      "hidden_size": 400,
      "bidir": false,
      "trainable_starter": "static"
    },
    "train": {
      "same_time_shuffle": false,
      "all_time_shuffle": true,
      "frooze_trx_encoder": false,
      "frooze_seq_encoder": false,
      "trx_dropout": 0.01,
      "max_seq_len": 800,
      "batch_size": 64,
      "num_workers": 16,
      "loss": "NLLLoss",
      "lr": 0.001,
      "weight_decay": 0.0,
      "n_epoch": 10
    },
    "lr_scheduler": {
      "step_size": 4,
      "step_gamma": 0.1
    },
    "valid": {
      "max_seq_len": 800,
      "batch_size": 512,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "accuracy",

    "pretrained_model_path": "models/mles_model_ft.p",
    "use_batch_norm": true
  },

  "save_model": false,

  "output": {
    "valid": {
      "path": "data/mles_finetuning_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/mles_finetuning_scores/test",
      "format": "pickle"
    },
    "columns": ["client_id"]
  }
}
