{
  "params": {
    "device": "cuda",

    "model_type": "rnn",
    "ensemble_size": 1,
    "trx_encoder": {
      "norm_embeddings": true,
      "embeddings_noise": 0.003,
      "embeddings": {
        "amino_acid": {"in": 25, "out": 8}
      },
      "numeric_values": {}
    },
    "rnn": {
      "hidden_size": 64,
      "type": "gru",
      "bidir": false,
      "trainable_starter": "static"
    },
    "head": {
      "pred_all_states": false,
      "pred_all_states_mean": false,
      "explicit_lengths": false,
      "norm_input": false,
      "use_batch_norm": true
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.01,
      "max_seq_len": 1200,
      "batch_size": 64,
      "num_workers": 16,
      "loss": "bce",
      "lr": 0.002,
      "weight_decay": 0.0,
      "n_epoch": 15
    },
    "lr_scheduler": {
      "step_size": 1,
      "step_gamma": 0.90
    },
    "valid": {
      "max_seq_len": 1200,
      "batch_size": 512,
      "num_workers": 8
    },
    "norm_scores": false
  },

  "save_model": false,

  "stats": {
    "path": "runs/fit_target.json"
  },

  "output": {
    "valid": {
      "path": "data/target_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/target_scores/test",
      "format": "pickle"
    },
    "columns": ["uid"]
  }
}
