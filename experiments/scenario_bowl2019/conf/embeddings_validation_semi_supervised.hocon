{
    environment: {
        work_dir: "embeddings_validation_semi_supervised.work"
    }
    report_file: "../results/scenario_bowl2019__semi_supervised.txt"

    target: {
        file_name: "../data/train_labels.csv"
        cols_id: [game_session]
        cols_id_type: [str]
        col_target: accuracy_group
    }

    split: {
        fit_ids: true

        train_id: {
            file_name: "../data/train_labels.csv"
        }
        test_id: {
            file_name: "../data/test_ids.csv"
        }

        cv_split_count: 5
        is_stratify: true
        random_state: 42
    }

    features: {
        baseline_00496: {
            enabled: true
            read_params: {file_name: ../data/agg_feat_embed.pickle}
            target_options: {labeled_amount: 496, random_state: 42}
        }
