        }
        baseline_07943: {
            enabled: true
            read_params: {file_name: ../data/agg_feat_embed.pickle}
            target_options: {labeled_amount: 7943, random_state: 42}
        }
        baseline_15887: {
            enabled: true
            read_params: {file_name: ../data/agg_feat_embed.pickle}
            target_options: {labeled_amount: 15885, random_state: 42}
        }

        mles_embeddings_00496: {
            enabled: true
            read_params: {file_name: ../data/mles_embeddings.pickle}
            target_options: {labeled_amount: 496, random_state: 42}
        }
        mles_embeddings_00994: {
            enabled: true
            read_params: {file_name: ../data/mles_embeddings.pickle}
            target_options: {labeled_amount: 994, random_state: 42}
        }
        mles_embeddings_01986: {
