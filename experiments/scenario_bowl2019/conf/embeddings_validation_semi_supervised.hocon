            read_params: {file_name: ../data/mles_embeddings.pickle}
            target_options: {labeled_amount: 3971, random_state: 42}
        }
        mles_embeddings_07943: {
            enabled: true
            read_params: {file_name: ../data/mles_embeddings.pickle}
            target_options: {labeled_amount: 7943, random_state: 42}
        }
        mles_embeddings_15887: {
