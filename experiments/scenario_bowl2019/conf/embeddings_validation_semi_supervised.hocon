    external_scores: {
        t_00496: "../results/fit_target_00496_results.json"
        m_00496: "../results/mles_finetuning_00496_results.json"
        c_00496: "../results/cpc_finetuning_00496_results.json"

        t_00994: "../results/fit_target_00994_results.json"
        m_00994: "../results/mles_finetuning_00994_results.json"
        c_00994: "../results/cpc_finetuning_00994_results.json"

        t_01986: "../results/fit_target_01986_results.json"
        m_01986: "../results/mles_finetuning_01986_results.json"
