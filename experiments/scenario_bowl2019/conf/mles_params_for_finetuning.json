{
  "params": {
    "experiment": {
      "name": "default"
    },
    "device": "cuda",
    "model_type": "rnn",
    "rnn": {
      "type": "gru",
      "hidden_size": 200,
      "bidir": false,
      "trainable_starter": "static"
    },
    "use_normalization_layer" : true,
    "trx_encoder": {
      "norm_embeddings": false,
      "embeddings_noise": 0.003,
      "embeddings": {
        "event_id": {"in": 500, "out": 16},
        "event_code": {"in": 50, "out": 8},
        "event_type": {"in": 6, "out": 8},
        "title": {"in": 50, "out": 8},
        "world": {"in": 6, "out": 8},
        "correct": {"in": 4, "out": 2}
      },
      "numeric_values": {
      }
    },
    "lr_scheduler": {
      "step_size": 30,
      "step_gamma": 0.9025
    },
    "train": {
      "num_workers": 16,
      "batch_size": 64,
      "split_strategy": {
        "split_strategy": "SampleSlices",
        "split_count": 5,
        "cnt_min": 25,
        "cnt_max": 150
      },
      "sampling_strategy": "HardNegativePair",
      "trx_dropout": 0.01,
      "max_seq_len": 5000,
      "neg_count": 5,
      "loss": "ContrastiveLoss",
      "margin": 0.5,
      "lr": 0.002,
      "weight_decay": 0.0,
      "n_epoch": 100
    },
    "norm_scores": false,
    "valid": {
      "batch_size": 1024,
      "num_workers": 16,
      "split_strategy": {
        "split_strategy": "SampleSlices",
        "split_count": 5,
        "cnt_min": 25,
        "cnt_max": 100
      },
      "max_seq_len": 5000
    }
  },

  "stats": {
    "path": "runs/bowl_ml_result.json"
  },

  "save_model": true,
  "model_path": {
    "model": "models/mles_model.p",
    "only_encoder": false
  },

  "output": {
    "path": "data/mles_embeddings",
    "format": "pickle",
    "columns": ["game_session"]
  }
}
