{
  "params": {
    "device": "cuda",
    "model_type": "rnn",
    "ensemble_size": 1,
    "trx_encoder": {
      "norm_embeddings": false,
      "embeddings_noise": 0.003,
      "embeddings": {
        "event_id": {"in": 500, "out": 16},
        "event_code": {"in": 50, "out": 8},
        "event_type": {"in": 6, "out": 8},
        "title": {"in": 50, "out": 8},
        "world": {"in": 6, "out": 8},
        "correct": {"in": 4, "out": 2}
      },
      "numeric_values": {
      }
    },
    "rnn": {
      "hidden_size": 32,
      "type": "gru",
      "bidir": false,
      "trainable_starter": "static"
    },
    "head": {
      "pred_all_states": false,
      "pred_all_states_mean": false,
      "explicit_lengths": false,
      "norm_input": false,
      "use_batch_norm": false,
      "objective": "regression"
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.005,
      "max_seq_len": 1000,
      "SubsamplingDataset": {
        "min_seq_len_s": 30,
        "min_seq_len_e": 500,
        "max_seq_len_s": 120,
        "max_seq_len_e": 1000,
        "crop_proba_init": 0.0,
        "crop_proba_gamma": 1.0,
        "total_n_epoch": 35
      },
      "batch_size": 64,
      "num_workers": 16,
      "loss": "mse",
      "lr": 0.01,
      "weight_decay": 0.0,
      "n_epoch": 35
    },
    "lr_scheduler": {
      "step_size": 5,
      "step_gamma": 0.5
    },
    "valid": {
      "max_seq_len": 5000,
      "batch_size": 64,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "kappa"
  },

  "save_model": false,

  "output": {
    "valid": {
      "path": "data/target_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/target_scores/test",
      "format": "pickle"
    },
    "columns": ["game_session"]
  }
}
