{
  "params": {
    "device": "cuda",

    "model_type": "rnn",
    "ensemble_size": 1,
    "trx_encoder": {
      "embeddings": {
        "event_id": {"in": 500},
        "event_code": {"in": 50},
        "event_type": {"in": 6},
        "title": {"in": 50},
        "world": {"in": 6},
        "correct": {"in": 4}
      },
      "numeric_values": {
      }
    },
    "rnn": {
      "hidden_size": 64
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.005,
      "max_seq_len": 5000,
      "SubsamplingDataset": {
        "min_seq_len_s": 500,
        "min_seq_len_e": 500,
        "max_seq_len_s": 1000,
        "max_seq_len_e": 1000,
        "crop_proba_init": 0.5,
        "crop_proba_gamma": 1.0,
        "total_n_epoch": 15
      },
      "batch_size": 64,
      "num_workers": 16,
      "loss": "NLLLoss",
      "lr": 0.001,
      "weight_decay": 0.0,
      "n_epoch": 10
    },
    "lr_scheduler": {
      "step_size": 4,
      "step_gamma": 0.1
    },
    "valid": {
      "max_seq_len": 5000,
      "batch_size": 512,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "accuracy",

    "pretrained_model_path": "models/mles_model.p",
    "use_batch_norm": true
  },

  "save_model": false,

  "stats": {
    "path": "runs/bowl2019_target_results.json"
  },

  "output": {
    "valid": {
      "path": "data/mles_finetuning_scores/valid",
      "format": "pickle"
    },
    "test": {
      "path": "data/mles_finetuning_scores/test",
      "format": "pickle"
    },
    "columns": ["game_session"]
  }
}
