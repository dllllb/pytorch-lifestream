{
  "params": {
    "device": "cuda",

    "model_type": "rnn",
    "ensemble_size": 1,
    "trx_encoder": {
      "embeddings": {
        "trans_date": {"in": 800},
        "small_group": {"in": 250}
      },
      "numeric_values": {
        "amount_rur": "identity"
      }
    },
    "rnn": {
      "hidden_size": 160
    },
    "head": {
      "pred_all_states": false,
      "pred_all_states_mean": false,
      "explicit_lengths": false,
      "norm_input": false,
      "num_classes": 4
    },
    "train": {
      "random_neg": false,
      "trx_dropout": 0.005,
      "max_seq_len": 1200,
      "SubsamplingDataset": {
        "min_seq_len_s": 30,
        "min_seq_len_e": 500,
        "max_seq_len_s": 120,
        "max_seq_len_e": 1000,
        "crop_proba_init": 1.0,
        "crop_proba_gamma": 1.0,
        "total_n_epoch": 35
      },
      "batch_size": 32,
      "num_workers": 16,
      "loss": "NLLLoss",
      "lr": 0.001,
      "weight_decay": 0.0,
      "n_epoch": 35
    },
    "lr_scheduler": {
      "step_size": 5,
      "step_gamma": 0.5
    },
    "valid": {
      "max_seq_len": 1200,
      "batch_size": 512,
      "num_workers": 8
    },
    "norm_scores": false,
    "score_metric": "accuracy",

    "pretrained_model_path": "models/age_pred_ml_model.p"
  },

  "save_model": false,

  "output": {
    "path": "../data/age-pred/fine_tuning_scores",
    "format": "pickle",
    "columns": ["client_id"]
  }
}
